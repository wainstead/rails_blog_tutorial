<h1>Listing articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
  </tr>

  <% @articles.each do |article| %>
  <tr>
    <td><%=  article.title %></td>
    <td><%=  article.text %></td>
    <td><%= link_to 'Show', article_patd(article) %></td>
  </tr>
  <% end %>
</table>
